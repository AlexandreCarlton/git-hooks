#!/bin/sh

# This hook is invoked after 'git checkout' succeeds.
# Note this also applies after a 'git clone'.

hooks_path="$(git config --path core.hooksPath || echo "${GIT_DIR}/hooks")"

"${hooks_path}/create-tags-file" &
"${hooks_path}/clean-python-bytecode-files" &
"${hooks_path}/link-ycm-extra-conf" &
